{
  "name": "backend-test",
  "version": "1.0.0",
  "description": "Backend engineer test - E1",
  "main": "src/server.ts",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "test:e2e": "node -r dotenv/config node_modules/.bin/jest --testPathPattern=e2e",
    "dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/server.ts",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/server.js",
    "start:prod": "wait-on tcp:tyba_postgres:5432 && npm run start",
    "start:migration": "wait-on tcp:tyba_postgres:5432 && node -r module-alias/register ./node_modules/typeorm/cli.js migration:run -d dist/config/database/database.config.js && npm run start",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/database/database.config.ts",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.override.yml up --build -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.24",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^11.1.0",
    "wait-on": "^8.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.2",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.29",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@entities": "dist/entities",
    "@migrations": "dist/migrations",
    "@routes": "dist/routes",
    "@repositories": "dist/repositories",
    "@controllers": "dist/controllers",
    "@services": "dist/services",
    "@utils": "dist/utils",
    "@middlewares": "dist/middlewares",
    "@dtos": "dist/dtos",
    "@common": "dist/common",
    "@test": "dist/test"
  }
}
